  "version": "0.2.0",
  "configurations": [
    {
      "name": "Rhino 8 - netcore",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "D:\\Program Files\\Rhino 8\\System\\Rhino.exe",
